{
	"name": "occurrence-count",
	"displayName": "Occurrence Count",
	"description": "Counts the occurrences of the selected word",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.count",
				"title": "Count Occurrences"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"keywords": ["productivity", "occurrences", "count", "words", "symbols"],
	"repository": {
		"type": "git",
		"url": "https://github.com/madprops/occurrence-count.git"
	},
	"icon": "icon.jpg"
}
